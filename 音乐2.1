-- è¾‰å…‰æ•ˆæœæ§åˆ¶å™¨
local GlowEffectController = {
    Enabled = true,
    Intensity = 1.0,
    PulseSpeed = 2,
    PulseIntensity = 0.3
}

function GlowEffectController:UpdateGlowEffect(uiElement)
    if not uiElement or not self.Enabled then return end
    
    local innerGlow = uiElement:GetAttribute("InnerGlow")
    local midGlow = uiElement:GetAttribute("MidGlow")
    local outerGlow = uiElement:GetAttribute("OuterGlow")
    
    if not innerGlow or not midGlow or not outerGlow then return end
    
    -- åˆ›å»ºè„‰åŠ¨æ•ˆæœ
    local time = tick()
    local pulse = math.sin(time * self.PulseSpeed) * self.PulseIntensity
    
    -- åŠ¨æ€è°ƒæ•´è¾‰å…‰å¼ºåº¦
    local baseIntensity = self.Intensity
    local dynamicIntensity = baseIntensity + pulse
    
    -- åº”ç”¨åŠ¨æ€æ•ˆæœåˆ°å„è¾‰å…‰å±‚
    innerGlow.Transparency = 0.7 - (dynamicIntensity * 0.1)
    innerGlow.Thickness = 4 + (dynamicIntensity * 1)
    
    midGlow.Transparency = 0.8 - (dynamicIntensity * 0.15)
    midGlow.Thickness = 6 + (dynamicIntensity * 1.5)
    
    outerGlow.Transparency = 0.85 - (dynamicIntensity * 0.2)
    outerGlow.Thickness = 8 + (dynamicIntensity * 2)
end

