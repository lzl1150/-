    -- ==================== å¢å¼ºçš„éŸ³ä¹æ§åˆ¶åŠŸèƒ½ ====================
    
    -- 1. å¢å¼ºçš„å£°éŸ³åç§°ç”Ÿæˆç³»ç»Ÿï¼ˆé˜²æ­¢æ¨¡å¼æ£€æµ‹ï¼‰
    local function generateNaturalName()
        local prefixes = {"EngineSound", "VehicleAudio", "AmbientTrack", "RadioStream", "EnvironmentSFX"}
        local suffixes = {"System", "Controller", "Manager", "Player", "Source", "Device"}
        local numbers = {"001", "002", "003", "004", "005"}
        return prefixes[math.random(#prefixes)].."_"..suffixes[math.random(#suffixes)].."_"..numbers[math.random(#numbers)]
    end
    
    local function generateMusicIDName(musicID)
        local patterns = {"Audio_%s", "Sound_%s", "Track_%s", "Media_%s"}
        return string.format(patterns[math.random(#patterns)], musicID:sub(-6))
    end
    
    local function generateRandomName()
        local chars = "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789"
        local name = ""
        for i = 1, math.random(10,15) do
            name = name .. string.sub(chars, math.random(1, #chars), math.random(1, #chars))
        end
        return name
    end
    
    -- å¢å¼ºçš„å£°éŸ³åç§°ç”Ÿæˆï¼ˆéšæœºé€‰æ‹©ç­–ç•¥ï¼‰
    local function generateEnhancedSoundName(musicID)
        local strategies = {
            function() return generateNaturalName() end,
            function() return generateRandomName() end,
            function() return generateMusicIDName(musicID) end
        }
        return strategies[math.random(#strategies)]()
    end
    
    -- 2. æ™ºèƒ½å‚æ•°éªŒè¯ä¸ä¼ªè£…ç³»ç»Ÿ
    local function ValidateAndCamouflageParameters(musicID, volume, pitch)
        -- æ¸…ç†éŸ³ä¹IDæ ¼å¼
        musicID = tostring(musicID):gsub("%D+", "")
        
        -- ä¸¥æ ¼é•¿åº¦æ£€æŸ¥
        if #musicID < 6 then
            return nil
        else
            musicID = musicID:sub(1, 10)
        end
        
        -- ä½¿ç”¨åˆç†èŒƒå›´å†…çš„å‚æ•°ï¼ˆé¿å…æç«¯å€¼ï¼‰
        local validVolume = math.clamp(tonumber(volume) or math.random(40, 70), 
            soundSystem.ProtectionBypass.ParameterRange.volume[1],
            soundSystem.ProtectionBypass.ParameterRange.volume[2])
        
        local validPitch = math.clamp(tonumber(pitch) or math.random(8, 15) * 0.1,
            soundSystem.ProtectionBypass.ParameterRange.pitch[1],
            soundSystem.ProtectionBypass.ParameterRange.pitch[2])
        
        -- æ·»åŠ å¾®å°éšæœºæ³¢åŠ¨ï¼ˆé¿å…å®Œå…¨ç›¸åŒçš„å‚æ•°ï¼‰
        validVolume = validVolume * (0.98 + math.random() * 0.04)
        validPitch = validPitch * (0.98 + math.random() * 0.04)
        
        return musicID, validVolume, validPitch
    end
    
    -- 3. æ—¶åºæ··æ·†ç³»ç»Ÿï¼ˆé˜²æ­¢æ¨¡å¼æ£€æµ‹ï¼‰
    local function applyRandomDelay()
        if soundSystem.ProtectionBypass.Enabled then
            local delay = math.random(soundSystem.ProtectionBypass.DelayRange[1], 
                soundSystem.ProtectionBypass.DelayRange[2]) / 1000
            task.wait(delay)
        end
    end
    
    -- 4. çˆ¶çº§ä½ç½®éšæœºåŒ–ï¼ˆå¢å¼ºéšè”½æ€§ï¼‰
    local function getRandomParentLocation()
        if soundSystem.ProtectionBypass.Enabled then
            return soundSystem.ProtectionBypass.ParentLocations[math.random(
                #soundSystem.ProtectionBypass.ParentLocations)]
        else
            return workspace
        end
    end
    
    -- 5. å¢å¼ºçš„è¿œç¨‹äº‹ä»¶æŸ¥æ‰¾ï¼ˆæ”¯æŒåˆ«åæ£€æµ‹ï¼‰
    local function findAC6RemoteEvents()
        local potentialRemotes = {}
        local validNames = {"AC6_FE_Sounds", "VehicleSounds", "AudioSystem", 
                           "SoundController", "MusicPlayer", "CarAudioSystem"}
        
        for _, location in ipairs({workspace, replicatedStorage}) do
            for _, obj in ipairs(location:GetDescendants()) do
                if obj:IsA("RemoteEvent") then
                    -- æ£€æŸ¥åç§°ç›¸ä¼¼åº¦
                    for _, validName in ipairs(validNames) do
                        if string.find(obj.Name:lower(), validName:lower()) or 
                           string.find(validName:lower(), obj.Name:lower()) then
                            table.insert(potentialRemotes, obj)
                            break
                        end
                    end
                end
            end
        end
        return potentialRemotes
    end
    
    local function SafeStopSound(soundID)
        local stopped = false
        for _, location in ipairs({workspace, replicatedStorage}) do
            for _, obj in ipairs(location:GetDescendants()) do
                if obj:IsA("RemoteEvent") and obj.Name == "AC6_FE_Sounds" then
                    pcall(function()
                        obj:FireServer("stopSound", soundID)
                        stopped = true
                    end)
                end
            end
        end
        return stopped
    end

    local function StopAllSounds()
        if #soundSystem.ActiveSounds == 0 then return end
        
        local soundsToStop = {}
        for _, soundID in ipairs(soundSystem.ActiveSounds) do
            table.insert(soundsToStop, soundID)
        end
        
        for _, soundID in ipairs(soundsToStop) do
            SafeStopSound(soundID)
        end
        
        soundSystem.ActiveSounds = {}
        soundSystem.LastSoundName = nil
    end

    -- 6. å¢å¼ºçš„æ’­æ”¾æ¨¡å¼ï¼ˆé›†æˆé˜²æŠ¤æªæ–½ï¼‰
    local function EnhancedMode1_Play(musicID, volume, pitch, loopEnabled)
        if soundSystem.RemoteEvent and not soundSystem.RemoteEvent:IsDescendantOf(game) then
            soundSystem.RemoteEvent = nil
        end
        
        if not soundSystem.RemoteEvent then
            local remotes = findAC6RemoteEvents()
            if #remotes > 0 then
                soundSystem.RemoteEvent = remotes[math.random(#remotes)]
            end
        end
        
        if not soundSystem.RemoteEvent then 
            warn("[AC6æ’­æ”¾å™¨] æœªæ‰¾åˆ°å¯ç”¨çš„è¿œç¨‹äº‹ä»¶")
            return false 
        end
        
        local soundName = generateEnhancedSoundName(musicID)
        local parentLocation = getRandomParentLocation()
        
        -- åº”ç”¨éšæœºå»¶è¿Ÿ
        applyRandomDelay()
        
        local success = pcall(function()
            soundSystem.RemoteEvent:FireServer("newSound", soundName, parentLocation, 
                              "rbxassetid://"..musicID, pitch, volume, loopEnabled)
            
            -- æ’­æ”¾å‰é¢å¤–å»¶è¿Ÿ
            applyRandomDelay()
            
            soundSystem.RemoteEvent:FireServer("playSound", soundName)
        end)
        
        if success then
            table.insert(soundSystem.ActiveSounds, soundName)
            soundSystem.LastSoundName = soundName
            return true
        else
            soundSystem.RemoteEvent = nil
            return false
        end
    end

    local function EnhancedMode2_Play(musicID, volume, pitch, loopEnabled)
        local soundName = generateEnhancedSoundName(musicID)
        local parentLocation = getRandomParentLocation()
        local playedSuccessfully = false
        local remoteEvents = findAC6RemoteEvents()
        
        if #remoteEvents == 0 then 
            warn("[AC6æ’­æ”¾å™¨] éå†æ¨¡å¼ï¼šæœªæ‰¾åˆ°ä»»ä½•è¿œç¨‹äº‹ä»¶")
            return false 
        end
        
        -- éšæœºæ‰“ä¹±è¿œç¨‹äº‹ä»¶é¡ºåº
        for i = #remoteEvents, 2, -1 do
            local j = math.random(i)
            remoteEvents[i], remoteEvents[j] = remoteEvents[j], remoteEvents[i]
        end
        
        for _, remote in ipairs(remoteEvents) do
            applyRandomDelay()
            
            local success = pcall(function()
                remote:FireServer("newSound", soundName, parentLocation, 
                                  "rbxassetid://"..musicID, pitch, volume, loopEnabled)
                
                applyRandomDelay()
                
                remote:FireServer("playSound", soundName)
            })
            
            if success then
                playedSuccessfully = true
                if not soundSystem.TwinMode then break end
            end
        end
        
        if playedSuccessfully then
            table.insert(soundSystem.ActiveSounds, soundName)
            soundSystem.LastSoundName = soundName
        end
        return playedSuccessfully
    end
    
    local function EnhancedAutoMode_Play(musicID, volume, pitch, loopEnabled)
        return EnhancedMode1_Play(musicID, volume, pitch, loopEnabled) or 
               EnhancedMode2_Play(musicID, volume, pitch, loopEnabled)
    end
    
    -- ==================== æ’­æ”¾æŒ‰é’®çŠ¶æ€åé¦ˆç³»ç»Ÿ ====================
    local function handlePlayback()
        local rawMusicID = musicIDBox.Text:gsub("%D+", "")
        local rawVolume = volumeBox.Text
        local rawPitch = pitchBox.Text
        
        local musicID, volume, pitch = ValidateAndCamouflageParameters(rawMusicID, rawVolume, rawPitch)
        
        -- å¦‚æœéŸ³ä¹IDæ— æ•ˆï¼Œç›´æ¥è§¦å‘å¤±è´¥çŠ¶æ€
        if not musicID then
            playButton.BackgroundColor3 = playButtonFailureColor
            task.delay(0.5, function()
                local tweenInfo = TweenInfo.new(0.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
                local tween = tweenService:Create(playButton, tweenInfo, {BackgroundColor3 = playButtonDefaultColor})
                tween:Play()
            end)
            return
        end
        
        local success = false
        if soundSystem.CurrentMode == "mode1" then
            success = EnhancedMode1_Play(musicID, volume, pitch, soundSystem.LoopEnabled)
        elseif soundSystem.CurrentMode == "mode2" then
            success = EnhancedMode2_Play(musicID, volume, pitch, soundSystem.LoopEnabled)
        else
            success = EnhancedAutoMode_Play(musicID, volume, pitch, soundSystem.LoopEnabled)
        end
        
        -- æ’­æ”¾æŒ‰é’®çŠ¶æ€é¢œè‰²åé¦ˆ
        if success then
            playButton.BackgroundColor3 = playButtonSuccessColor
            print("[AC6æ’­æ”¾å™¨] éŸ³ä¹æ’­æ”¾æˆåŠŸ - ID:", musicID, "éŸ³é‡:", volume, "éŸ³è°ƒ:", pitch)
        else
            playButton.BackgroundColor3 = playButtonFailureColor
            warn("[AC6æ’­æ”¾å™¨] éŸ³ä¹æ’­æ”¾å¤±è´¥ - è¯·å°è¯•åˆ‡æ¢æ¨¡å¼æˆ–æ£€æŸ¥æ¸¸æˆæ”¯æŒ")
